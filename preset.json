{
  "useConfigFiles": true,
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-typescript": {
      "classComponent": true,
      "useTsWithBabel": true
    }
  },
  "configs": {
    "vue": {
      "css": {
        "modules": true
      }
    },
    "eslintConfig": {
      "rules": {
        "semi": "off"
      }
    }
  }
}
